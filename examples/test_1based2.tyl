// Test 1-based indexing with assignment
fn main:
    var arr = [10, 20, 30]
    
    println "Before: arr[1] = {arr[1]}, arr[2] = {arr[2]}, arr[3] = {arr[3]}"
    
    // Modify using 1-based index
    arr[1] = 100
    arr[3] = 300
    
    println "After: arr[1] = {arr[1]}, arr[2] = {arr[2]}, arr[3] = {arr[3]}"
    
    // Store in variables
    let first = arr[1]
    let second = arr[2]
    let third = arr[3]
    
    println "first = {first}, second = {second}, third = {third}"

main()
