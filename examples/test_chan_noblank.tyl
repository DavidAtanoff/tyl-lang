// Channel test no blank

fn test_channel_timeout:
    print("--- Test Channel Timeout ---\n")
    let ch = chan[int, 1]
    print("Buffered channel created\n")
    print("Sending value...\n")
    ch <- 100
    print("Sent value 100 to channel\n")
    let value = <- ch
    print("Received value: ")
    print(value)
    print("\n")

fn main -> int:
    test_channel_timeout()
    return 0
