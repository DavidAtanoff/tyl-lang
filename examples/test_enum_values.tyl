// Test enums with explicit values
enum Status:
    Ok = 0
    Error = -1
    Pending = 1
    Timeout = 2

enum Color:
    Red = 1
    Green = 2
    Blue = 4

fn main:
    let s = Status.Ok
    let e = Status.Error
    let p = Status.Pending
    
    println("Status.Ok = ", s)
    println("Status.Error = ", e)
    println("Status.Pending = ", p)
    
    let r = Color.Red
    let g = Color.Green
    let b = Color.Blue
    
    println("Color.Red = ", r)
    println("Color.Green = ", g)
    println("Color.Blue = ", b)
    
    // Test enum comparison
    if s == 0:
        println("Status is Ok!")
    
    println("Enums with values work!")
