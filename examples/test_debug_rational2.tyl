// Debug rational issue - narrowing down
fn main() {
    print("Test: Negative BigInt then rational\n")
    let b3 = bigint(-50)
    let b4 = bigint(150)
    let bs2 = bigint_add(b3, b4)
    let bi2 = bigint_to_int(bs2)
    print("BigInt result: ")
    print(bi2)
    print("\n")
    
    print("\nRational after negative BigInt:\n")
    let r3 = rational(1, 3)
    let f3 = rational_to_float(r3)
    print("1/3 = ")
    print(f3)
    print("\n")
}
