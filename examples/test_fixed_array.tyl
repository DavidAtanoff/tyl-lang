// Test fixed-size arrays
fn main:
    // Fixed-size array declaration
    let arr: [int; 5] = [1, 2, 3, 4, 5]
    
    println("Fixed array test:")
    println("arr[0] = ", arr[0])
    println("arr[4] = ", arr[4])
    
    // Modify array elements
    mut arr2: [int; 3] = [10, 20, 30]
    arr2[1] = 25
    println("arr2[1] = ", arr2[1])
    
    println("Fixed-size arrays work!")
