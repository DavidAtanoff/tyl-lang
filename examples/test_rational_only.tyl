// Test Rational only
fn main() {
    print("--- Rational Tests ---\n")
    let rat_half = rational(1, 2)
    let rat_third = rational(1, 3)
    let rat_sum = rational_add(rat_half, rat_third)
    print("Rational: 1/2 + 1/3 = ")
