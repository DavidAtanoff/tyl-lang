// Test 3: Comparison and Logical Operators
// Testing: ==, !=, <, >, <=, >=, and, or, not

a = 10
b = 20
c = 10

println("=== Comparison Test ===")
println("a = {a}, b = {b}, c = {c}")

// Equality
eq1 = a == c
eq2 = a == b
println("a == c: {eq1}")
println("a == b: {eq2}")

// Inequality
ne1 = a != b
ne2 = a != c
println("a != b: {ne1}")
println("a != c: {ne2}")

// Less than
lt1 = a < b
lt2 = b < a
println("a < b: {lt1}")
println("b < a: {lt2}")

// Greater than
gt1 = b > a
gt2 = a > b
println("b > a: {gt1}")
println("a > b: {gt2}")

// Less than or equal
le1 = a <= c
le2 = a <= b
println("a <= c: {le1}")
println("a <= b: {le2}")

// Greater than or equal
ge1 = a >= c
ge2 = b >= a
println("a >= c: {ge1}")
println("b >= a: {ge2}")

// Logical operators
t = true
f = false

and1 = t and t
and2 = t and f
println("true and true: {and1}")
println("true and false: {and2}")

or1 = t or f
or2 = f or f
println("true or false: {or1}")
println("false or false: {or2}")

not1 = not t
not2 = not f
println("not true: {not1}")
println("not false: {not2}")

println("Test 3 PASSED")
