// Adding more loops
println("Start")

for j in 0..2:
    println("j = {j}")

for k in range(3):
    println("k = {k}")

// Break
for n in 0..10:
    if n == 2:
        break
    println("n = {n}")

// Continue
for p in 0..4:
    if p % 2 == 0:
        continue
    println("p = {p}")

println("End")
