// Minimal crash test - combining features
println("Start")

x = 15
if x > 20:
    println("x > 20")
elif x > 10:
    println("x > 10")
else:
    println("x <= 10")

// Nested if
y = 5
if y > 0:
    if y < 10:
        println("y is between 0 and 10")

// While loop
mut i = 1
while i <= 3:
    println("i = {i}")
    i += 1

// For loops
for j in 0..2:
    println("j = {j}")

for k in range(3):
    println("k = {k}")

// Break
for n in 0..10:
    if n == 2:
        break
    println("n = {n}")

// Continue
for p in 0..4:
    if p % 2 == 0:
        continue
    println("p = {p}")

println("End")
