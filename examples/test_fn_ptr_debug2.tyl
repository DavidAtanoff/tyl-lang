// Debug function pointer call - simpler

fn double_it x: int -> int:
    println("In double_it, x = ", x)
    return x * 2

fn apply_fn x: int, y: int -> int:
    println("In apply_fn")
    println("x = ", x)
    println("y = ", y)
    return x + y

fn main:
    println("Calling apply_fn")
    result = apply_fn(21, 42)
    println("Result = ", result)
