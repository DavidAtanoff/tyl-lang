// Debug: Positive then negative BigInt adds then rational
fn main() {
    print("First BigInt add (positive):\n")
    let b1 = bigint(100)
    let b2 = bigint(200)
    let bs1 = bigint_add(b1, b2)
    let bi1 = bigint_to_int(bs1)
    print("Result: ")
    print(bi1)
    print("\n")
    
    print("\nSecond BigInt add (with negative):\n")
    let b3 = bigint(-50)
    let b4 = bigint(150)
    let bs2 = bigint_add(b3, b4)
    let bi2 = bigint_to_int(bs2)
    print("Result: ")
    print(bi2)
    print("\n")
    
    print("\nRational after both:\n")
    let r = rational(1, 3)
    let f = rational_to_float(r)
    print("1/3 = ")
    print(f)
    print("\n")
}
