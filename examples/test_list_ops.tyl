// Test List Operations
println("=== List Operations Test ===")

// Create a list
numbers = [10, 20, 30, 40, 50]
println("Created list: numbers")

// Access by index
first = numbers[0]
println("First element: {first}")

third = numbers[2]
println("Third element: {third}")

last = numbers[4]
println("Last element: {last}")

// Get length
size = len(numbers)
println("List size: {size}")

// Iterate over list
println("Iterating over list:")
for n in numbers:
    println("  Value: {n}")

println("List Operations Test PASSED")
