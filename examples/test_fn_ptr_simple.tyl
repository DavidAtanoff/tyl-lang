// Test simple function pointer parameter

fn apply_fn x: int, f: fn(int) -> int -> int:
    println("In apply_fn, x = ", x)
    println("About to call f...")
    result = f(x)
    println("f returned: ", result)
    return result

fn double_it x: int -> int:
    println("In double_it, x = ", x)
    return x * 2

fn main:
    println("Calling apply_fn")
    result = apply_fn(21, double_it)
    println("Result = ", result)
