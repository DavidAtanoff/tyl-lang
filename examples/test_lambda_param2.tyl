// Test passing lambda as parameter with explicit type

fn apply_fn x: int, f: fn(int) -> int -> int:
    println("In apply_fn, x = ", x)
    println("About to call f...")
    result = f(x)
    println("f returned: ", result)
    return result

fn double_it x: int -> int:
    return x * 2

fn main:
    println("Step 1: Calling apply_fn with named function")
    result = apply_fn(21, double_it)
    println("Step 2: Result = ", result)
