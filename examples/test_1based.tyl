// Test 1-based indexing
fn main:
    let arr = [10, 20, 30, 40, 50]
    
    // 1-based indexing: arr[1] is the first element
    println "arr[1] = {arr[1]}"  // Should print 10
    println "arr[2] = {arr[2]}"  // Should print 20
    println "arr[3] = {arr[3]}"  // Should print 30
    println "arr[4] = {arr[4]}"  // Should print 40
    println "arr[5] = {arr[5]}"  // Should print 50

main()
