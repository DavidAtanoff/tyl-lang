// Test type conversion functions with unique variable names

println("=== Testing int() ===")
a = int("42")
println("int('42') = ", a)

b = int("-123")
println("int('-123') = ", b)

c = int("  456  ")
println("int('  456  ') = ", c)

y = 3.7
d = int(y)
println("int(3.7) = ", d)

println("int(true) = ", int(true))
println("int(false) = ", int(false))

println("")
println("=== Testing float() ===")
f1 = float(42)
println("float(42) = ", f1)

f2 = float("3.14159")
println("float('3.14159') = ", f2)

f3 = float(-10)
println("float(-10) = ", f3)

println("")
println("=== Testing bool() ===")
println("bool(0) = ", bool(0))
println("bool(1) = ", bool(1))
println("bool(42) = ", bool(42))

println("")
println("=== Testing str() ===")
s1 = str(12345)
println("str(12345) = ", s1)

s2 = str(-999)
println("str(-999) = ", s2)

