// Test 6: Ternary and Null Coalescing
// Testing: ternary operator, null coalescing

println("=== Ternary and Null Coalescing Test ===")

// Ternary operator
x = 10
result1 = x > 5 ? "greater" : "smaller"
println("10 > 5 ? 'greater' : 'smaller' = {result1}")

result2 = x < 5 ? "less" : "not less"
println("10 < 5 ? 'less' : 'not less' = {result2}")

// Ternary with numbers
a = 15
b = 20
max_val = a > b ? a : b
println("max(15, 20) = {max_val}")

min_val = a < b ? a : b
println("min(15, 20) = {min_val}")

// Nested ternary
score = 75
grade = score >= 90 ? "A" : score >= 80 ? "B" : score >= 70 ? "C" : "F"
println("Score 75 grade = {grade}")

// Null coalescing
value1 = nil
default1 = value1 ?? 42
println("nil ?? 42 = {default1}")

value2 = 100
default2 = value2 ?? 42
println("100 ?? 42 = {default2}")

println("Test 6 PASSED")
