// Test range behavior
// .. is INCLUSIVE (1..5 = 1,2,3,4,5)
// range() is EXCLUSIVE like Python (range(1,5) = 1,2,3,4)

fn main:
    print("Range 1..5 (inclusive - should print 1,2,3,4,5):")
    for i in 1..5:
        print(i)
    
    print("Range 0..3 (inclusive - should print 0,1,2,3):")
    for i in 0..3:
        print(i)
    
    print("range(5) (exclusive - should print 0,1,2,3,4):")
    for i in range(5):
        print(i)
    
    print("range(1,5) (exclusive - should print 1,2,3,4):")
    for i in range(1, 5):
        print(i)
