// Test multi-dimensional fixed-size arrays

fn main:
    println("=== Multi-Dimensional Array Test ===")
    
    // Test 1D array first
    println("")
    println("1D array [int; 3]:")
    mut arr1d: [int; 3]
    arr1d[0] = 10
    arr1d[1] = 20
    arr1d[2] = 30
    println("  arr1d[0] = ", arr1d[0])
    println("  arr1d[1] = ", arr1d[1])
    println("  arr1d[2] = ", arr1d[2])
    
    // Test nested 1D arrays (simulating 2D)
    println("")
    println("Nested 1D arrays (simulating 2D):")
    mut row0: [int; 3]
    mut row1: [int; 3]
    row0[0] = 1
    row0[1] = 2
    row0[2] = 3
    row1[0] = 4
    row1[1] = 5
    row1[2] = 6
    println("  row0: ", row0[0], ", ", row0[1], ", ", row0[2])
    println("  row1: ", row1[0], ", ", row1[1], ", ", row1[2])
    
    println("")
    println("Multi-dimensional array test complete!")
