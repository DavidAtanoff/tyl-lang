// Test 7: Built-in Functions
// Testing: print, println, len, str, upper, contains, platform, arch

println("=== Built-in Functions Test ===")

// String functions
s = "hello"
println("String: {s}")
println("len('hello') = {len(s)}")
println("upper('hello') = {upper(s)}")

// Contains
has_ell = contains("hello", "ell")
has_xyz = contains("hello", "xyz")
println("contains('hello', 'ell') = {has_ell}")
println("contains('hello', 'xyz') = {has_xyz}")

// str() conversion
num = 42
numStr = str(num)
println("str(42) = {numStr}")

// Platform info
println("platform() = {platform()}")
println("arch() = {arch()}")

// System info
println("hostname() = {hostname()}")
println("username() = {username()}")
println("cpu_count() = {cpu_count()}")

// Time functions
println("year() = {year()}")
println("month() = {month()}")
println("day() = {day()}")
println("hour() = {hour()}")
println("minute() = {minute()}")
println("second() = {second()}")

// List length
numbers = [1, 2, 3, 4, 5]
println("len([1,2,3,4,5]) = {len(numbers)}")

println("Test 7 PASSED")
