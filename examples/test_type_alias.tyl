// Test type aliases
type size_t = int
type count_t = int
type handle_t = int

fn main:
    let x: size_t = 42
    let count: count_t = 100
    let handle: handle_t = 12345
    
    println("size_t value: ", x)
    println("count_t value: ", count)
    println("handle_t value: ", handle)
    println("Type aliases work!")
