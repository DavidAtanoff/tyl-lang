// Test string operations - simpler version

text = "Hello World"
hw = "Hello"
ww = "World"

// Test starts_with
result = starts_with(text, hw)
println("starts_with Hello: {result}")

result2 = starts_with(text, ww)
println("starts_with World: {result2}")

// Test ends_with
result3 = ends_with(text, ww)
println("ends_with World: {result3}")

// Test index_of
idx = index_of(text, ww)
println("index_of World: {idx}")

idx2 = index_of(text, "xyz")
println("index_of xyz: {idx2}")
