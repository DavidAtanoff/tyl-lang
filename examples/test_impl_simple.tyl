// Test simple impl without generics

record Point:
    x: int
    y: int

impl Point:
    fn add self: Point, other: Point -> Point:
        println("In Point.add")
        println("self.x = ", self.x)
        println("self.y = ", self.y)
        println("other.x = ", other.x)
        println("other.y = ", other.y)
        return Point { x: self.x + other.x, y: self.y + other.y }

fn main:
    p1 = Point { x: 10, y: 20 }
    p2 = Point { x: 5, y: 15 }
    println("p1 = (", p1.x, ", ", p1.y, ")")
    println("p2 = (", p2.x, ", ", p2.y, ")")
    println("Calling p1.add(p2)")
    p3 = p1.add(p2)
    println("p3 = (", p3.x, ", ", p3.y, ")")
