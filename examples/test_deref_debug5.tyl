// Debug - immutable vs mutable

fn test_immut:
    x = 42
    println("immutable x = {x}")

fn test_mut:
    mut x = 42
    println("mutable x = {x}")

fn test_mut_modify:
    mut x = 42
    x = 100
    println("modified x = {x}")

fn main:
    test_immut()
    test_mut()
    test_mut_modify()

main()
