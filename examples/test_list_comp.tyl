// Test List Comprehensions
println("=== List Comprehension Test ===")

// Simple list comprehension with range
squares = [x * x for x in 0..5]
println("Created squares list")

// Access elements
println("squares[0] = {squares[0]}")
println("squares[1] = {squares[1]}")
println("squares[2] = {squares[2]}")
println("squares[3] = {squares[3]}")
println("squares[4] = {squares[4]}")
println("squares[5] = {squares[5]}")

// List comprehension with range()
doubles = [n * 2 for n in range(5)]
println("Created doubles list")
println("doubles[0] = {doubles[0]}")
println("doubles[1] = {doubles[1]}")
println("doubles[2] = {doubles[2]}")

println("List Comprehension Test PASSED")
